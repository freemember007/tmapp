<Alloy>
	<Collection src="feed"/>
	<Window onOpen="fetchFeed" title='今天'>
		<TableView id="table" dataCollection="feed">
			<TableViewSection id="date" headerTitle="{date}">
				<TableViewRow id="row">
					<View id="rowContainer">
						<ImageView id="image" image="{image}"/ >
						<Label id="content" text="{content}"/>
					</View>
				</TableViewRow>
			</TableViewSection>
		</TableView>
	</Window>
</Alloy>